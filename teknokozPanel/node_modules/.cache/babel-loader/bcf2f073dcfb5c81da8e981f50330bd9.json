puted","getEditCategory","get","mounted"],"sourceRoot":"","sources":["/Users/gurhankoz/Desktop/teknokozpanel/src/views/Category/categoryEditView.vue"],"sourcesContent":["<template>\n  <div class=\"divNewItem\">\n    <div class=\"mb-3\">\n      <label for=\"exampleFormControlInput1\" class=\"form-label\"> Kategori Adı</label>\n      <input type=\"email\" class=\"form-control\" id=\"exampleFormControlInput1\" :value=\"this.editCategory.name\">\n    </div>\n    <div class=\"mb-3\">\n      <label for=\"formFile\" class=\"form-label\">Resim</label>\n      <input class=\"form-control\" type=\"file\" id=\"formFile\" ref=\"file\" @change=\"uploadFile\">\n    </div>\n    <div class=\"mb-3\">\n      <label for=\"exampleFormControlTextarea1\" class=\"form-label\">İçerik</label>\n      <textarea class=\"form-control\" id=\"exampleFormControlTextarea1\" rows=\"3\" :value=\"this.editCategory.content\"></textarea>\n    </div>\n    <div class=\"newItemBtnGroup\">\n      <button class=\"btn kytBtn\" @click=\"updateCategory\">Düzenle</button>\n      <router-link\n          to=\"/category\"\n          class=\"btn closeBtn\"\n      >\n        İptal\n      </router-link>\n    </div>\n  </div>\n\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"categoryEditView\",\n  data(){\n    return{\n      editCategory:{},\n      name : null,\n      image1 : null,\n      content: null,\n    }\n  },\n  methods:{\n    updateCategory(){\n      let formdata = new FormData();\n      if (this.image1 !== null){\n        formdata.append('image1',this.image1)\n      }\n      formdata.append('name', this.name)\n      formdata.append('content', this.content)\n      axios.put('http://127.0.0.1:8000/api/category/'+this.$route.params.id, formdata\n          ,{\n            headers: {\n              Authorization: \"Bearer \" + localStorage.getItem('token'),\n              'Content-Type': \"multipart/form-data; charset=utf-8\",\n            },\n          }).then(response => {\n        this.$swal.fire({\n          position: 'top-end',\n          icon: 'success',\n          title: response.data,\n          showConfirmButton: false,\n          timer: 1500\n        });\n        setTimeout(function(){\n          window.location.reload();\n        },1600)\n      }).catch(error => {\n        this.$swal.fire({\n          position: 'top-end',\n          icon: 'error',\n          title: error,\n          showConfirmButton: false,\n          timer: 1500\n        });\n      })\n    },\n    uploadFile(e) {\n      if(this.$refs.file.files[0] !== null){\n        this.image1  = this.$refs.file.files[0]\n      }else {\n        this.image1  = null\n      }\n    },\n  },\n  computed:{\n    getEditCategory(){\n      axios.get('http://127.0.0.1:8000/api/category/' + this.$route.params.id,\n          {\n            headers: {\n              Authorization: \"Bearer \" + localStorage.getItem('token'),\n            }\n          }).then(response => {\n        this.editCategory = response.data\n      })\n    }\n  },\n  mounted() {\n    this.getEditCategory;\n  }\n}\n</script>\n\n<style scoped>\n.newItemBtnGroup{\n  text-align: end;\n}\n.btn{\n  border: none;\n  transition: transform .2s;\n}\n.kytBtn{\n  background-color: coral;\n  font-size: 15px;\n  margin: 8px;\n}\n.closeBtn{\n  background-color: #EB1D36;\n  font-size: 15px;\n  margin: 8px;\n}\n.kytBtn:hover{\n  transform: scale(1.1);\n}\n.closeBtn:hover{\n  transform: scale(1.1);\n}\n</style>"]},"metadata":{},"sourceType":"module"}