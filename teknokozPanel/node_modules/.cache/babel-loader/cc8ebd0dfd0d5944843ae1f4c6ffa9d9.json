ass=\"mb-3\">\n      <label for=\"formFile\" class=\"form-label\">Resim</label>\n      <input class=\"form-control\" type=\"file\" id=\"formFile\" @change=\"uploadFile\">\n    </div>\n    <div class=\"mb-3\">\n      <label for=\"exampleFormControlTextarea1\" class=\"form-label\">İçerik</label>\n      <textarea class=\"form-control\" id=\"exampleFormControlTextarea1\" rows=\"3\" v-model=\"content\"></textarea>\n    </div>\n    <div class=\"newItemBtnGroup\">\n      <button class=\"btn kytBtn\" type=\"submit\">Kaydet</button>\n      <router-link\n          to=\"/category\"\n          class=\"btn closeBtn\"\n      >\n        İptal\n      </router-link>\n    </div>\n  </div>\n  </form>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"newCategoryView\",\n  data(){\n    return{\n        name: null,\n        image1: null,\n        content: null,\n    }\n  },\n   methods:{\n    createNewCategory(){\n      axios.post('http://127.0.0.1:8000/api/category',\n          {\n            name: this.name,\n            content: this.content,\n            image1: this.image1\n          },{\n            headers: {\n              Authorization: \"Bearer \" + localStorage.getItem('token'),\n              Accept: application/json,\n            },\n          })\n    },\n    uploadFile(e) {\n      this.image1 = e.target.files[0]\n      console.log(this.image1)\n    }\n    },\n}\n</script>\n\n<style scoped>\n.newItemBtnGroup{\n  text-align: end;\n}\n.btn{\n  border: none;\n  transition: transform .2s;\n}\n.kytBtn{\n  background-color: #B1D7B4;\n  font-size: 15px;\n  margin: 8px;\n}\n.closeBtn{\n  background-color: #EB1D36;\n  font-size: 15px;\n  margin: 8px;\n}\n.kytBtn:hover{\n  transform: scale(1.1);\n}\n.closeBtn:hover{\n  transform: scale(1.1);\n}\n</style>"]},"metadata":{},"sourceType":"module"}