\" type=\"text\" placeholder=\"Ara..\">\n      <router-link\n          to=\"/newCategory\"\n          class=\"thPlus btn\"\n      >\n        <i class=\"fa-solid fa-plus\"></i>\n      </router-link>\n\n    </div>\n    <table class=\"table table-striped\">\n      <thead>\n      <tr>\n        <th scope=\"col\">Id</th>\n        <th scope=\"col\">Kategori</th>\n        <th scope=\"col\">İçerik</th>\n        <th scope=\"col\">Resim</th>\n      </tr>\n      </thead>\n      <tbody>\n\n\n\n      <tr v-for=\"category in categories\">\n        <th scope=\"row\">{{category.id}}</th>\n        <td>{{category.name}}</td>\n        <td class=\"tdcontent\">{{category.content}}</td>\n        <td class=\"tdimg\"><img class=\"img\" :src=\"category.image\" alt=\"\"></td>\n        <td class=\"tdbutton\">\n          <router-link\n              to=\"/categoryEdit\"\n              class=\"btn-edit btn\"\n          >\n            <i class=\"fa-regular fa-pen-to-square\"></i>\n          </router-link>\n          <button class=\"btn btn-del\" @click=\"deleteItem(category.id)\">\n            <i class=\"fa-regular fa-trash-can\"></i>\n          </button>\n        </td>\n      </tr>\n\n\n\n\n      </tbody>\n    </table>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport router from \"@/router\";\nexport default {\n  name: \"categoryView\",\n  data(){\n    return{\n      categories: [],\n    }\n  },\n  methods:{\n    deleteItem(id){\n      this.$swal.fire({\n        title: 'Do you want to save the changes?',\n        showDenyButton: true,\n        showCancelButton: true,\n        confirmButtonText: 'Save',\n        denyButtonText: `Don't save`,\n      }).then((result) => {\n        /* Read more about isConfirmed, isDenied below */\n        if (result.isConfirmed) {\n          Swal.fire('Saved!', '', 'success')\n        } else if (result.isDenied) {\n          Swal.fire('Changes are not saved', '', 'info')\n        }\n      });\n\n      axios.delete('http://127.0.0.1:8000/api/category/'+id,\n          {\n            headers: {\n              Authorization: \"Bearer \" + localStorage.getItem('token'),\n            },\n          }).then(response => {\n        this.$swal.fire({\n          position: 'top-end',\n          icon: 'success',\n          title: response.data,\n          showConfirmButton: false,\n          timer: 1500\n        });\n        setTimeout(function(){\n          window.location.reload();\n        },1600)\n\n      }).catch(error => {\n        this.$swal.fire({\n          position: 'top-end',\n          icon: 'error',\n          title: error,\n          showConfirmButton: false,\n          timer: 1500\n        });\n      })\n    },\n  },\n  computed:{\n    getcategory(){\n        axios.get('http://127.0.0.1:8000/api/category',\n        {\n          headers: {\n            Authorization: \"Bearer \" + localStorage.getItem('token'),\n          }\n        }).then(response => {\n          this.categories = response.data;\n    })\n  }\n  },\n  mounted() {\n    this.getcategory;\n  }\n}\n</script>\n\n<style scoped>\n.divTable{\n  overflow: auto;\n}\ntable{\n  border-collapse: unset!important;\n}\ntable tr{\n  width: 100px;\n  height: 50px;\n}\ntable th{\n  font-weight: 500;\n  font-family: 'Optima';\n}\ntable td{\n  max-width: 150px;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n}\n\n.tdimg{\n  text-align: center;\n  overflow: unset;\n}\n.img{\n  width: 60px;\n  object-fit: cover;\n  transition: transform .8s;\n  border-radius: 5px;\n}\n.tdbutton{\n  width: 100px;\n  text-align: center;\n  border: none;\n}\n.btn{\n  border: none;\n  font-size: 20px;\n  transition: transform .2s;\n}\n.tableSp{\n  margin-top: 10px;\n  margin-right: 10px;\n  text-align: end;\n}\n.thSearch{\n  width: 200px;\n  height: 40px;\n  border: none;\n  border-radius: 5px;\n  padding: 3px;\n  text-align: center;\n  border-left: 4px solid coral ;\n}\n.thPlus{\n  border: none;\n  font-size: 20px;\n  background-color: #B1D7B4;\n  margin-left: 15px;\n}\n.thPlus:hover{\n  transform: scale(1.3);\n}\n.thSearch:focus{\n  border-left: 4px solid #a1c4fd;\n}\n.thSearch:focus-visible{\n  outline: none;\n}\n.btn-edit:hover{\n  color: coral;\n  transform: scale(1.3);\n  border: none;\n}\n.btn-del:hover{\n  color: red;\n  transform: scale(1.3);\n  border: none;\n}\n.img:hover{\n  transform: scale(3);\n  border: 0.5px solid #fff;\n  border-radius: 5px;\n  -webkit-transform-origin-y: bottom;\n  -webkit-transform-origin-x: right;\n}\n</style>"]},"metadata":{},"sourceType":"module"}