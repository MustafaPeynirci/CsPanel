{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"categoryEditView\",\n\n  data() {\n    return {\n      editCategory: {}\n    };\n  },\n\n  methods: {\n    updateCategory() {\n      let categoryForm = new FormData();\n\n      if (this.editCategory.image1 !== null) {\n        categoryForm.append('image1', this.editCategory.image);\n      }\n\n      categoryForm.append('name', this.editCategory.name);\n      categoryForm.append('content', this.editCategory.content);\n      categoryForm.append('_method', 'PUT');\n      console.log(this.editCategory.name);\n      console.log(this.editCategory.content);\n      console.log(categoryForm);\n      axios.put('http://127.0.0.1:8000/api/category/' + this.$route.params.id, this.categoryForm, {\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem('token'),\n          'Content-Type': \"multipart/form-data; charset=utf-8\"\n        }\n      }).then(response => {\n        console.log(categoryForm);\n        this.$swal.fire({\n          position: 'top-end',\n          icon: 'success',\n          title: response.data,\n          showConfirmButton: false,\n          timer: 1500\n        });\n        setTimeout(function () {//window.location.reload();\n        }, 1600);\n      }).catch(error => {\n        this.$swal.fire({\n          position: 'top-end',\n          icon: 'error',\n          title: error,\n          showConfirmButton: false,\n          timer: 1500\n        });\n      });\n    },\n\n    uploadFile(e) {\n      if (this.$refs.file.files[0] !== null) {\n        this.editCategory.image1 = this.$refs.file.files[0];\n      } else {\n        this.editCategory.image1 = null;\n      }\n    }\n\n  },\n  computed: {\n    getEditCategory() {\n      axios.get('http://127.0.0.1:8000/api/category/' + this.$route.params.id, {\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem('token')\n        }\n      }).then(response => {\n        this.editCategory = response.data;\n      });\n    }\n\n  },\n\n  mounted() {\n    this.getEditCategory;\n  }\n\n};","map":{"version":3,"mappings":"AA4BA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACbC,IAAI,EAAE,kBADO;;EAEbC,IAAI,GAAE;IACJ,OAAM;MACJC,YAAY,EAAC;IADT,CAAN;EAGD,CANY;;EAObC,OAAO,EAAC;IACNC,cAAc,GAAE;MACd,IAAIC,YAAW,GAAI,IAAIC,QAAJ,EAAnB;;MACA,IAAI,KAAKJ,YAAL,CAAkBK,MAAlB,KAA6B,IAAjC,EAAsC;QACpCF,YAAY,CAACG,MAAb,CAAoB,QAApB,EAA6B,KAAKN,YAAL,CAAkBO,KAA/C;MACF;;MACAJ,YAAY,CAACG,MAAb,CAAoB,MAApB,EAA4B,KAAKN,YAAL,CAAkBF,IAA9C;MACAK,YAAY,CAACG,MAAb,CAAoB,SAApB,EAA+B,KAAKN,YAAL,CAAkBQ,OAAjD;MACAL,YAAY,CAACG,MAAb,CAAoB,SAApB,EAA+B,KAA/B;MACAG,OAAO,CAACC,GAAR,CAAY,KAAKV,YAAL,CAAkBF,IAA9B;MACAW,OAAO,CAACC,GAAR,CAAY,KAAKV,YAAL,CAAkBQ,OAA9B;MACAC,OAAO,CAACC,GAAR,CAAYP,YAAZ;MACAN,KAAK,CAACc,GAAN,CAAU,wCAAsC,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,EAAnE,EAGI,KAAKX,YAHT,EAMI;QACEY,OAAO,EAAE;UACPC,aAAa,EAAE,YAAYC,YAAY,CAACC,OAAb,CAAqB,OAArB,CADpB;UAEP,gBAAgB;QAFT;MADX,CANJ,EAWOC,IAXP,CAWYC,QAAO,IAAK;QAClBX,OAAO,CAACC,GAAR,CAAYP,YAAZ;QACJ,KAAKkB,KAAL,CAAWC,IAAX,CAAgB;UACdC,QAAQ,EAAE,SADI;UAEdC,IAAI,EAAE,SAFQ;UAGdC,KAAK,EAAEL,QAAQ,CAACrB,IAHF;UAId2B,iBAAiB,EAAE,KAJL;UAKdC,KAAK,EAAE;QALO,CAAhB;QAOAC,UAAU,CAAC,YAAU,CACnB;QACD,CAFS,EAER,IAFQ,CAAV;MAGD,CAvBD,EAuBGC,KAvBH,CAuBSC,KAAI,IAAK;QAChB,KAAKT,KAAL,CAAWC,IAAX,CAAgB;UACdC,QAAQ,EAAE,SADI;UAEdC,IAAI,EAAE,OAFQ;UAGdC,KAAK,EAAEK,KAHO;UAIdJ,iBAAiB,EAAE,KAJL;UAKdC,KAAK,EAAE;QALO,CAAhB;MAOD,CA/BD;IAgCD,CA5CK;;IA6CNI,UAAU,CAACC,CAAD,EAAI;MACZ,IAAG,KAAKC,KAAL,CAAWC,IAAX,CAAgBC,KAAhB,CAAsB,CAAtB,MAA6B,IAAhC,EAAqC;QACnC,KAAKnC,YAAL,CAAkBK,MAAlB,GAA4B,KAAK4B,KAAL,CAAWC,IAAX,CAAgBC,KAAhB,CAAsB,CAAtB,CAA5B;MACD,CAFD,MAEM;QACJ,KAAKnC,YAAL,CAAkBK,MAAlB,GAA4B,IAA5B;MACF;IACD;;EAnDK,CAPK;EA4Db+B,QAAQ,EAAC;IACPC,eAAe,GAAE;MACfxC,KAAK,CAACyC,GAAN,CAAU,wCAAwC,KAAK1B,MAAL,CAAYC,MAAZ,CAAmBC,EAArE,EACI;QACEC,OAAO,EAAE;UACPC,aAAa,EAAE,YAAYC,YAAY,CAACC,OAAb,CAAqB,OAArB;QADpB;MADX,CADJ,EAKOC,IALP,CAKYC,QAAO,IAAK;QACtB,KAAKpB,YAAL,GAAoBoB,QAAQ,CAACrB,IAA7B;MACD,CAPD;IAQF;;EAVO,CA5DI;;EA