rol\" id=\"exampleFormControlTextarea1\" rows=\"3\"></textarea>\n    </div>\n    <div class=\"newItemBtnGroup\">\n      <button class=\"btn kytBtn\">Düzenle</button>\n      <router-link\n          to=\"/category\"\n          class=\"btn closeBtn\"\n      >\n        İptal\n      </router-link>\n    </div>\n  </div>\n\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"categoryEditView\",\n  data(){\n    return{\n      editCategory:{}\n    }\n  },\n  computed:{\n    getEditCategory(){\n      axios.get('http://127.0.0.1:8000/api/category/' + this.$route.params.id,\n          {\n            headers: {\n              Authorization: \"Bearer \" + localStorage.getItem('token'),\n            }\n          }).then(response => {\n        console.log(response)\n        this.editCategory = response.data\n      })\n    }\n  },\n  mounted() {\n    this.getEditCategory;\n  }\n}\n</script>\n\n<style scoped>\n.newItemBtnGroup{\n  text-align: end;\n}\n.btn{\n  border: none;\n  transition: transform .2s;\n}\n.kytBtn{\n  background-color: coral;\n  font-size: 15px;\n  margin: 8px;\n}\n.closeBtn{\n  background-color: #EB1D36;\n  font-size: 15px;\n  margin: 8px;\n}\n.kytBtn:hover{\n  transform: scale(1.1);\n}\n.closeBtn:hover{\n  transform: scale(1.1);\n}\n</style>"]},"metadata":{},"sourceType":"module"}