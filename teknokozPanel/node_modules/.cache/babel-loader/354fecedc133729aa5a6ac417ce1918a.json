,{\n            headers: {\n              Authorization: \"Bearer \" + localStorage.getItem('token'),\n              Accept : \"application/json\",\n            },\n          }).then(response => {\n        console.log(response.data)\n        this.categories = response.data;\n      })\n    },\n    uploadFile(e) {\n      this.newCategory.image1 = e.target.files[0]\n    }\n    },\n}\n</script>\n\n<style scoped>\n.newItemBtnGroup{\n  text-align: end;\n}\n.btn{\n  border: none;\n  transition: transform .2s;\n}\n.kytBtn{\n  background-color: #B1D7B4;\n  font-size: 15px;\n  margin: 8px;\n}\n.closeBtn{\n  background-color: #EB1D36;\n  font-size: 15px;\n  margin: 8px;\n}\n.kytBtn:hover{\n  transform: scale(1.1);\n}\n.closeBtn:hover{\n  transform: scale(1.1);\n}\n</style>"]},"metadata":{},"sourceType":"module"}