{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"newCategoryView\",\n\n  data() {\n    return {\n      newCategory: {\n        name: '',\n        image1: '',\n        content: ''\n      }\n    };\n  },\n\n  methods: {\n    createNewCategory() {\n      let formdata = new FormData();\n\n      if (this.newCategory.image1 !== null) {\n        formdata.append('image1', this.newCategory.image1);\n      }\n\n      formdata.append('name', this.newCategory.name);\n      formdata.append('content', this.newCategory.content);\n      axios.post('http://127.0.0.1:8000/api/category', formdata, {\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem('token'),\n          'Content-Type': \"multipart/form-data; charset=utf-8\"\n        }\n      }).then(response => {\n        this.$swal.fire({\n          position: 'top-end',\n          icon: 'success',\n          title: response.data,\n          showConfirmButton: false,\n          timer: 1500\n        });\n        setTimeout(function () {\n          window.location.reload();\n        }, 1600);\n      }).catch(error => {\n        this.$swal.fire({\n          position: 'top-end',\n          icon: 'error',\n          title: 'Boş Alan Bırakmayınız!',\n          showConfirmButton: false,\n          timer: 1500\n        });\n      });\n    },\n\n    uploadFile(e) {\n      if (this.$refs.file.files[0] !== '') {\n        this.newCategory.image1 = this.$refs.file.files[0];\n      } else {\n        this.newCategory.image1 = '';\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6BA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACbC,IAAI,EAAE,iBADO;;EAEbC,IAAI,GAAE;IACJ,OAAM;MACJC,WAAW,EAAC;QACVF,IAAI,EAAE,EADI;QAEVG,MAAM,EAAE,EAFE;QAGVC,OAAO,EAAE;MAHC;IADR,CAAN;EAOD,CAVY;;EAWZC,OAAO,EAAC;IACPC,iBAAiB,GAAE;MACjB,IAAIC,QAAO,GAAI,IAAIC,QAAJ,EAAf;;MACA,IAAI,KAAKN,WAAL,CAAiBC,MAAjB,KAA4B,IAAhC,EAAqC;QACnCI,QAAQ,CAACE,MAAT,CAAgB,QAAhB,EAAyB,KAAKP,WAAL,CAAiBC,MAA1C;MACF;;MACAI,QAAQ,CAACE,MAAT,CAAgB