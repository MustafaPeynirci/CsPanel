ile.files[0]\n      }\n    },\n  },\n  computed:{\n    getEditCategory(){\n      axios.get('http://127.0.0.1:8000/api/category/' + this.$route.params.id,\n          {\n            headers: {\n              Authorization: \"Bearer \" + localStorage.getItem('token'),\n            }\n          }).then(response => {\n          this.editCategory.name = response.data.name;\n          this.editCategory.content = response.data.content;\n          this.editCategory.image = response.data.image;\n          console.log(this.editCategory.image)\n      })\n    }\n  },\n  mounted() {\n    this.getEditCategory;\n  }\n}\n</script>\n\n<style scoped>\n.newItemBtnGroup{\n  text-align: end;\n}\n.btn{\n  border: none;\n  transition: transform .2s;\n}\n.kytBtn{\n  background-color: coral;\n  font-size: 15px;\n  margin: 8px;\n}\n.closeBtn{\n  background-color: #EB1D36;\n  font-size: 15px;\n  margin: 8px;\n}\n.kytBtn:hover{\n  transform: scale(1.1);\n}\n.closeBtn:hover{\n  transform: scale(1.1);\n}\n</style>"]},"metadata":{},"sourceType":"module"}