{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"categoryEditView\",\n\n  data() {\n    return {\n      editCategory: {}\n    };\n  },\n\n  methods: {\n    updateCategory() {\n      let formdata = new FormData();\n\n      if (this.editCategory.image1 !== null) {\n        formdata.append('image1', this.editCategory.image);\n      }\n\n      formdata.append('name', this.editCategory.name);\n      formdata.append('content', this.editCategory.content);\n      formdata.append('_method', \"PUT\");\n      axios.post('http://127.0.0.1:8000/api/category/' + this.$route.params.id, {\n        formdata\n      }, {\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem('token'),\n          'Content-Type': \"multipart/form-data; charset=utf-8\"\n        }\n      }).then(response => {\n        console.log(formdata);\n        this.$swal.fire({\n          position: 'top-end',\n          icon: 'success',\n          title: response.data,\n          showConfirmButton: false,\n          timer: 1500\n        });\n        setTimeout(function () {//window.location.reload();\n        }, 1600);\n      }).catch(error => {\n        this.$swal.fire({\n          position: 'top-end',\n          icon: 'error',\n          title: error,\n          showConfirmButton: false,\n          timer: 1500\n        });\n      });\n    },\n\n    uploadFile(e) {\n      if (this.$refs.file.files[0] !== null) {\n        this.editCategory.image1 = this.$refs.file.files[0];\n      } else {\n        this.image1 = null;\n      }\n    }\n\n  },\n  computed: {\n    getEditCategory() {\n      axios.get('http://127.0.0.1:8000/api/category/' + this.$route.params.id, {\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem('token')\n        }\n      }).then(response => {\n        this.editCategory = response.data;\n      });\n    }\n\n  },\n\n  mounted() {\n    this.getEditCategory;\n  }\n\n};","map":{"version":3,"mappings":"AA4BA,OAAOA,KAAP,MA