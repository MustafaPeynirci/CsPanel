  {\n            headers: {\n              Authorization: \"Bearer \" + localStorage.getItem('token'),\n              'Content-Type': \"multipart/form-data; charset=utf-8\",\n            },\n          }).then(response => {\n            console.log(formdata)\n        this.$swal.fire({\n          position: 'top-end',\n          icon: 'success',\n          title: response.data,\n          showConfirmButton: false,\n          timer: 1500\n        });\n        setTimeout(function(){\n          //window.location.reload();\n        },1600)\n      }).catch(error => {\n        this.$swal.fire({\n          position: 'top-end',\n          icon: 'error',\n          title: error,\n          showConfirmButton: false,\n          timer: 1500\n        });\n      })\n    },\n    uploadFile(e) {\n      if(this.$refs.file.files[0] !== null){\n        this.editCategory.image1  = this.$refs.file.files[0]\n      }else {\n        this.image1  = null\n      }\n    },\n  },\n  computed:{\n    getEditCategory(){\n      axios.get('http://127.0.0.1:8000/api/category/' + this.$route.params.id,\n          {\n            headers: {\n              Authorization: \"Bearer \" + localStorage.getItem('token'),\n            }\n          }).then(response => {\n        this.editCategory = response.data\n      })\n    }\n  },\n  mounted() {\n    this.getEditCategory;\n  }\n}\n</script>\n\n<style scoped>\n.newItemBtnGroup{\n  text-align: end;\n}\n.btn{\n  border: none;\n  transition: transform .2s;\n}\n.kytBtn{\n  background-color: coral;\n  font-size: 15px;\n  margin: 8px;\n}\n.closeBtn{\n  background-color: #EB1D36;\n  font-size: 15px;\n  margin: 8px;\n}\n.kytBtn:hover{\n  transform: scale(1.1);\n}\n.closeBtn:hover{\n  transform: scale(1.1);\n}\n</style>"]},"metadata":{},"sourceType":"module"}